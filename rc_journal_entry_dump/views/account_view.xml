<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="view_tree_account_move_dump" model="ir.ui.view">
            <field name="name">view.tree.account.move.dump</field>
            <field name="model">account.move.dump</field>
            <field name="arch" type="xml">
                <tree string="Account Move Dump" create="false" edit="false" delete="false">
                    <field name="name" />
                    <field name="code" />
                    <field name="document_date" />
                    <field name="tnx_code" />
                    <field name="document_number" />
                    <field name="reference_number" />
                    <field name="division" />
                    <field name="department" />
                    <field name="narration" />
                    <field name="debit" />
                    <field name="credit" />
                    <field name="balance" />
                </tree>
            </field>
        </record>

        <record id="account_move_dump_view_form" model="ir.ui.view">
            <field name="name">account.move.dump.view.form</field>
            <field name="model">account.move.dump</field>
            <field name="arch" type="xml">
                <form string="Account Move Dump" create="false" delete="false" edit="false">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" />
                                <field name="code" />
                                <field name="document_date" />
                                <field name="tnx_code" />
                                <field name="document_number" />
                                <field name="reference_number" />
                            </group>
                            <group>
                                <field name="division" />
                                <field name="department" />
                                <field name="narration" />
                                <field name="debit" />
                                <field name="credit" />
                                <field name="balance" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="account_move_dump_view_search" model="ir.ui.view">
            <field name="name">account.move.dump.view.search</field>
            <field name="model">account.move.dump</field>
            <field name="arch" type="xml">
                <search string="Account Dump">
                    <field name="name" />
                    <field name="code" />
                    <field name="document_date" />
                    <field name="tnx_code" />
                    <field name="document_number" />
                    <field name="reference_number" />
                    <field name="division" />
                    <field name="department" />
                    <field name="narration" />
                    <field name="debit" />
                    <field name="credit" />
                    <field name="balance" />
                </search>
            </field>
        </record>

        <record id="account_move_dump_action" model="ir.actions.act_window">
            <field name="name">Account Move Dump</field>
            <field name="res_model">account.move.dump</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="oe_no_content_create">Add an entry</p>
            </field>
        </record>

        <menuitem id="menu_account_dump_root" name="History" parent="account.menu_finance_configuration" />

        <menuitem id="menu_account_dump_transaction" name="Transactions" parent="menu_account_dump_root" action="account_move_dump_action" />

    </data>
</odoo>
