                                                <t>
                                                    <td>
                                                        <span t-esc="ol.name"/>
                                                    </td>
                                                    <td>
                                                        <span t-field="ol.start_date" t-options='{"widget": "datetime"}'/>
                                                    </td>
                                                    <td>
                                                        <span t-esc="ol.end_date" t-options='{"widget": "datetime"}'/>
                                                    </td>
                                                    <td>
                                                        <span t-field="ol.assigned_resource"/>
