                                <h4>Project Implementation Plan</h4>
                                <table class="table table-sm">
                                    <thead class="bg-100">
                                        <tr>
                                            <th>Task</th>
                                            <th>Start Date &amp; Time</th>
                                            <th>End Date &amp; Time</th>
                                            <th>Assigned Resource(s)</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <t t-foreach="device_onboarding.project_plan_line_ids" t-as="ol">
